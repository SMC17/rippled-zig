{
  "schema_version": 1,
  "cases": {
    "account_info_missing_param": {
      "request": "{\"method\":\"account_info\",\"params\":{}}",
      "expected_error": "Invalid account_info params"
    },
    "account_info_invalid_account": {
      "request": "{\"method\":\"account_info\",\"params\":{\"account\":\"invalid\"}}",
      "expected_error": "Invalid account_info params"
    },
    "submit_missing_blob": {
      "request": "{\"method\":\"submit\",\"params\":{}}",
      "expected_error": "Invalid submit params"
    },
    "submit_empty_blob": {
      "request": "{\"method\":\"submit\",\"params\":{\"tx_blob\":\"\"}}",
      "expected_error": "Invalid submit params"
    },
    "submit_non_hex_blob": {
      "request": "{\"method\":\"submit\",\"params\":{\"tx_blob\":\"DEADZEEF\"}}",
      "expected_error": "Invalid submit params"
    },
    "submit_invalid_blob_structure": {
      "request": "{\"method\":\"submit\",\"params\":{\"tx_blob\":\"DEADBEEF\"}}",
      "expected_error": "Invalid submit tx_blob"
    },
    "submit_unsupported_tx_type": {
      "request": "{\"method\":\"submit\",\"params\":{\"tx_blob\":\"FFFF0101010101010101010101010101010101010101000000000000000A00000005\"}}",
      "expected_error": "Unsupported submit transaction type"
    },
    "submit_account_set_wrong_length": {
      "request": "{\"method\":\"submit\",\"params\":{\"tx_blob\":\"00030101010101010101010101010101010101010101000000000000000A00000005010101010101010101010101010101010101010101000000000000000A\"}}",
      "expected_error": "Invalid submit tx_blob"
    },
    "submit_missing_destination_account": {
      "request": "{\"method\":\"submit\",\"params\":{\"tx_blob\":\"00000101010101010101010101010101010101010101000000000000000A00000008030303030303030303030303030303030303030300000000000003E8\"}}",
      "expected_error": "Submit destination account not found"
    },
    "submit_insufficient_payment_balance": {
      "request": "{\"method\":\"submit\",\"params\":{\"tx_blob\":\"00000101010101010101010101010101010101010101000000000000000A000000080202020202020202020202020202020202020202000000000BEBC200\"}}",
      "expected_error": "Insufficient submit payment balance"
    },
    "submit_blocked_in_production": {
      "request": "{\"method\":\"submit\",\"params\":{\"tx_blob\":\"DEADBEEF\"}}",
      "expected_error": "Method blocked by profile policy"
    },
    "agent_config_set_blocked_in_production": {
      "request": "{\"method\":\"agent_config_set\",\"params\":{\"key\":\"max_peers\",\"value\":30}}",
      "expected_error": "Method blocked by profile policy"
    },
    "random_blocked_in_production": {
      "request": "{\"method\":\"random\"}",
      "expected_error": "Method blocked by profile policy"
    }
  }
}
